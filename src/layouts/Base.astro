---
import "../styles/global.css";
import BaseHead from "../components/BaseHead.astro";

interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="es">
	<BaseHead title={title} description="My first page" />

	<body class="h-screen">
		<slot />
	</body>
</html>
